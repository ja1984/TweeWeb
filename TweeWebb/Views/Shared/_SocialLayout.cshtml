<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" name="viewport" content="width=device-width" />
        <link rel="shortcut icon" type="image/x-icon" href="/Content/images/icon.png" />
        <link rel="icon" type="image/x-icon" href="/Content/images/icon.png" />
        <title>@ViewBag.Title</title>
        <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
        <link href="/Content/Social/Site.css" rel="stylesheet" />
    </head>
<body>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-40833760-1', 'twee-app.com');
        ga('send', 'pageview');
    </script>
   


@RenderBody()
    
    
    <div style="clear: both;"></div>
    <div style="text-align: center;"><a href="https://plus.google.com/107604673419334925296" rel="publisher">Google+</a></div>
    
    <script src="/Scripts/jquery-1.8.2.js"></script>
    <script src="/Scripts/masonry.pkgd.min.js"></script>
    <script type="text/javascript">
        $(function () {
            var $container = $('#container');
            // initialize
            $container.masonry({
                itemSelector: '.box'
            });
            $(".seasonwrapper").click(function() {
                var episodeList = $(this).next();
                episodeList.slideToggle(400, function () { $container.masonry(); });
                
            });
        });
        

        

    </script>

</body>
</html>
